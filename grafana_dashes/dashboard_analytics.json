  "title": "Analytics (MongoDB)",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "5m",
  "panels": [
    {
      "type": "timeseries",
      "title": "User Stats: Прослушивания vs Пропуски",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "grafana-mongodb-datasource", "uid": "MongoDB" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "grafana-mongodb-datasource", "uid": "MongoDB" },
          "database": "db",
          "collection": "user_stats",
          "isPipeline": true,
          "pipeline": "[{\"$group\":{\"_id\":{\"$dateToString\":{\"format\":\"%Y-%m-%d\",\"date\":\"$timestamp\"}},\"value\":{\"$sum\":\"$tracks_listened\"}}},{\"$sort\":{\"_id\":1}}]",
          "format": "time_series"
        },
        {
          "refId": "B",
          "datasource": { "type": "grafana-mongodb-datasource", "uid": "MongoDB" },
          "database": "db",
          "collection": "user_stats",
          "isPipeline": true,
          "pipeline": "[{\"$group\":{\"_id\":{\"$dateToString\":{\"format\":\"%Y-%m-%d\",\"date\":\"$timestamp\"}},\"value\":{\"$sum\":\"$tracks_skipped\"}}},{\"$sort\":{\"_id\":1}}]",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      }
    },
    {
      "type": "barchart",
      "title": "Top 10 Played Tracks (7 дней)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "grafana-mongodb-datasource", "uid": "MongoDB" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "grafana-mongodb-datasource", "uid": "MongoDB" },
          "database": "db",
          "collection": "top_played_tracks",
          "isPipeline": true,
          "pipeline": "[{\"$match\":{\"timestamp\":{\"$gte\":{\"$date\":{\"$subtract\":[\"$$NOW\",604800000]}}}}},{\"$group\":{\"_id\":\"$track_id\",\"totalPlays\":{\"$sum\":\"$play_count\"}}},{\"$sort\":{\"totalPlays\":-1}},{\"$limit\":10}]",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "showValue": "auto",
        "legend": { "displayMode": "list", "placement": "bottom" }
      }
    },
